#! /usr/bin/vvp
:ivl_version "10.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xf41340 .scope module, "hello" "hello" 2 1;
 .timescale 0 0;
    .scope S_0xf41340;
T_0 ;
    %vpi_call 2 4 "$display", "Hello Mario" {0 0 0};
    %vpi_call 2 5 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "hello.v";
